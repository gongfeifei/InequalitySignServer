<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.inequalitysignserver">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.CheckActivity" />
        <activity android:name=".ui.ManagerActivity" />
        <activity android:name=".ui.TypeActivity" />
        <activity android:name=".ui.CallActivity" />
        <activity android:name=".ui.InquiryActivity" />
        <activity android:name=".ui.ManagerNameActivity" />
        <activity android:name=".ui.ManagerAddressActivity" />
        <activity android:name=".ui.ManagerTelActivity" />
        <activity android:name=".ui.ClipImageActivity" />
        <activity android:name=".ui.SettingActivity"></activity>
    </application>

</manifest>